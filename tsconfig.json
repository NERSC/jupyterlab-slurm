{
  "compilerOptions": {
    "declaration": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5",
    "outDir": "./lib",
    "lib": ["ES5", "ES2015.Promise", "DOM"],
    // "typeRoots": ["node_modules/@types", "custom_typings", "."],
    "types": []
  },
  "include": ["src/*"]
}
